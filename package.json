{
  "name": "zoom-clone",
  "version": "1.0.0",
  "description": "Video Chat application ",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon --exec babel-node server.js",
    "build": "npm run clean && babel . --ignore node_modules --out-dir build",
